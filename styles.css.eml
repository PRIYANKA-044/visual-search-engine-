Content-Type: text/plain; charset="UTF-8"

body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
    text-align: center;
    background-color: #f4f4f9;
}

#app {
    max-width: 800px;
    margin: 50px auto;
    padding: 20px;
    background: white;
    box-shadow: 0px 2px 8px rgba(0, 0, 0, 0.1);
    border-radius: 8px;
}

input[type="text"], input[type="file"] {
    padding: 10px;
    margin: 10px;
}

#results {
    margin-top: 20px;
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));
    gap: 10px;
}
img {
    width: 100%;
    height: auto;
    border-radius: 5px;
}

